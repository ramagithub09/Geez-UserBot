@register(outgoing=True, pattern='^.r(?: |$)(.*)')
async def typewriter(typew):
    typew.pattern_match.group(1)
    await typew.edit("`rawrrr`")
    sleep(1)
    await typew.edit("`sedang berpikir...`")
    sleep(1)
    await typew.edit("`mulai memindai...`")
    sleep(1)
    await typew.edit("`Error 404`")
